{
  "manifest_version": 3,
  "name": "ChatGPT WebSocket URL Extractor",
  "version": "1.0",
  "description": "Extract URLs from ChatGPT WebSocket delta messages in real-time.",
  "permissions": ["activeTab", "scripting", "storage"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://chat.openai.com/c/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}


